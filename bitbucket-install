rm -rf /maven/bitbucket-home
rm -rf /maven/atlassian-bitbucket-4.10.1/
mkdir /maven/bitbucket-home
cd /maven/
tar -zxvf atlassian-bitbucket-4.10.1.tar.gz

cd /maven/atlassian-bitbucket-4.10.1/bin/ modify set-bitbucket-home.sh

/maven/atlassian-bitbucket-4.10.1/bin/start-bitbucket.sh
/maven/atlassian-bitbucket-4.10.1/bin/stop-bitbucket.sh

